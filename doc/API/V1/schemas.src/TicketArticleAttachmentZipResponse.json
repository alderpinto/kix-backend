{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#TicketArticleAttachmentZipResponse",
  "type": "object",
  "description": "A response for ticket article attachments as zip GET request.",
  "properties": {
    "Attachment": {
      "properties": {
        "Content": {
          "type": "string",
          "readOnly": true,
          "description": "added via include \"Content\" (base64 encoded zip content)"
        },
        "ContentType": {
            "type": "string",
            "readOnly": true
        },
        "Filename": {
            "type": "string",
            "readOnly": true
        },
        "Filesize": {
            "type": "string",
            "readOnly": true
        },
        "FilesizeRaw": {
            "type": "string",
            "readOnly": true
        }
      },
      "required" : [
        "ContentType",
        "Filename",
        "Filesize",
        "FilesizeRaw"
      ]
    }
  },
  "required": [
    "Attachment"
  ]
}