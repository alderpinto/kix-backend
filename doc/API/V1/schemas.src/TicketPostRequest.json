{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "TicketPostRequest.json",
  "type": "object",
  "description": "The ticket request object for creation.",
  "properties": {
    "Ticket": {
      "type": "object",
      "properties": {
        "Articles": {
          "type": "array",
          "item": {
            "$ref": "TicketArticlePostItem.json"
          }
        },
        "ContactID": {
          "type": "integer"
        },
        "DynamicFields": {
          "type": "array",
          "description": "list of dynamic field values to be set",
          "items": {
            "$ref": "DynamicFieldValueSetItem.json"
          }
        },
        "LockID": {
          "type": "integer"
        },
        "OrganisationID": {
          "type": "integer"
        },
        "OwnerID": {
          "type": "integer"
        },
        "PendingTime": {
          "type": "string"
        },
        "PriorityID": {
          "type": "integer"
        },
        "QueueID": {
          "type": "integer"
        },
        "ResponsibleID": {
          "type": "integer"
        },
        "StateID": {
          "type": "integer"
        },
        "Title": {
          "type": "string"
        },
        "TypeID": {
          "type": "integer"
        }
      },
      "required": [
        "ContactID",
        "Title"
      ]
    }
  },
  "required": [
    "Ticket"
  ]
}