{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "TicketPatchRequest.json",
  "type": "object",
  "description": "The ticket request object for update.",
  "properties": {
    "Ticket": {
      "type": "object",
      "properties": {
        "ContactID": {
          "type": "integer"
        },
        "DynamicFields": {
          "type": "array",
          "description": "list of dynamic field values to be set (if no new value given, an already exiting value will be unchanged)",
          "items": {
            "$ref": "DynamicFieldValueSetItem.json"
          }
        },
        "LockID": {
          "type": "integer"
        },
        "OrganisationID": {
          "type": "integer"
        },
        "OwnerID": {
          "type": "integer"
        },
        "PendingTime": {
          "type": "string"
        },
        "PriorityID": {
          "type": "integer"
        },
        "QueueID": {
          "type": "integer"
        },
        "ResponsibleID": {
          "type": "integer"
        },
        "ServiceID": {
          "type": "integer"
        },
        "StateID": {
          "type": "integer"
        },
        "TimeUnit" : {
          "type" : "integer",
          "description": "Value will be combined with existing value for this ticket (negative values are possible)."
        },
        "Title": {
          "type": "string"
        },
        "TypeID": {
          "type": "integer"
        }
      }
    }
  },
  "required": [
    "Ticket"
  ]
}