{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#DynamicFieldValue",
  "type": "object",
  "description": "The dynamic field value object.",
  "properties": {
    "DisplayValue": {
      "type": "string",
      "description": "prepared values ",
      "readOnly": true
    },
    "DisplayValueHTML": {
      "type": "string",
      "readOnly": true
    },
    "DisplayValueShort": {
      "type": "string",
      "readOnly": true
    },
    "ID": {
      "type": "integer",
      "description": "ID of the dynamic field",
      "readOnly": true
    },
    "Label": {
      "type": "string",
      "description": "label of the dynamic field",
      "readOnly": true
    },
    "Name": {
      "type": "string",
      "description": "name of the dynamic field",
      "readOnly": true
    },
    "PreparedValue": {
      "type": "array",
      "description": "prepared values - e.g. display values of a selection field instead of the keys",
      "items": {
        "type": "string"
      },
      "readOnly": true
    },
    "Value": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "readOnly": true
    }
  },
  "required": [
    "DisplayValue",
    "DisplayValueHTML",
    "DisplayValueShort",
    "ID",
    "Label",
    "Name",
    "PreparedValue",
    "Value"
  ]
}