{
   "$id" : "ContactPostRequest.json",
   "$schema" : "http://json-schema.org/draft-07/schema",
   "description" : "The contact request object for creation.",
   "type" : "object",
   "properties" : {
      "Contact" : {
         "type" : "object",
         "properties" : {
            "AssignedUserID" : {
               "description" : "Id of an user, but should not be already in use for another contact.",
               "type" : "integer"
            },
            "City" : {
               "type" : "string"
            },
            "Comment" : {
               "type" : "string"
            },
            "Country" : {
               "type" : "string"
            },
            "Email" : {
               "type" : "string"
            },
            "Fax" : {
               "type" : "string"
            },
            "Firstname" : {
               "type" : "string"
            },
            "Lastname" : {
               "type" : "string"
            },
            "Mobile" : {
               "type" : "string"
            },
            "OrganisationIDs" : {
               "type" : "array",
               "items" : {
                  "type" : "integer"
               }
            },
            "Phone" : {
               "type" : "string"
            },
            "PrimaryOrganisationID" : {
               "type" : "integer"
            },
            "Street" : {
               "type" : "string"
            },
            "Title" : {
               "type" : "string"
            },
            "ValidID" : {
               "type" : "integer"
            },
            "Zip" : {
               "type" : "string"
            }
         },
         "required" : [
            "Firstname",
            "Lastname",
            "OrganisationIDs",
            "PrimaryOrganisationID"
         ]
      }
   },
   "required" : [
      "Contact"
   ]
}
