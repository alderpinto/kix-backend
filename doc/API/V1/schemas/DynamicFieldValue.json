{
   "$id" : "DynamicFieldValue.json",
   "$schema" : "http://json-schema.org/draft-07/schema",
   "description" : "The dynamic field value object.",
   "type" : "object",
   "properties" : {
      "DisplayValue" : {
         "description" : "prepared values ",
         "type" : "string",
         "readOnly" : true
      },
      "DisplayValueHTML" : {
         "type" : "string",
         "readOnly" : true
      },
      "DisplayValueShort" : {
         "type" : "string",
         "readOnly" : true
      },
      "ID" : {
         "description" : "ID of the dynamic field",
         "type" : "integer",
         "readOnly" : true
      },
      "Label" : {
         "description" : "label of the dynamic field",
         "type" : "string",
         "readOnly" : true
      },
      "Name" : {
         "description" : "name of the dynamic field",
         "type" : "string",
         "readOnly" : true
      },
      "PreparedValue" : {
         "description" : "prepared values - e.g. display values of a selection field instead of the keys",
         "type" : "array",
         "items" : {
            "type" : "string"
         },
         "readOnly" : true
      },
      "Value" : {
         "type" : "array",
         "items" : {
            "type" : "string"
         },
         "readOnly" : true
      }
   },
   "required" : [
      "DisplayValue",
      "DisplayValueHTML",
      "DisplayValueShort",
      "ID",
      "Label",
      "Name",
      "PreparedValue",
      "Value"
   ]
}
