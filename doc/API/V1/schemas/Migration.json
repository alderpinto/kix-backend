{
   "$id" : "MigrationState.json",
   "$schema" : "http://json-schema.org/draft-07/schema",
   "description" : "The migration state object.",
   "type" : "object",
   "properties" : {
      "ID" : {
         "type" : "string",
         "readOnly" : true
      },
      "State" : {
         "description" : "The content depends on the source of the migration.",
         "type" : "object",
         "patternProperties" : {
            "^[a-zA-Z]+$" : {
               "type" : "object",
               "items" : {
                  "anyOf" : [
                     {
                        "type" : "string"
                     },
                     {
                        "type" : "integer"
                     }
                  ]
               }
            }
         },
         "readOnly" : true
      },
      "Status" : {
         "type" : "string",
         "readOnly" : true
      }
   },
   "required" : [
      "ID",
      "Status"
   ]
}
