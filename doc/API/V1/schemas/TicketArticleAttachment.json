{
   "$schema": "http://json-schema.org/draft-07/schema",
   "$id": "TicketArticleAttachment.json",
   "type": "object",
   "description": "The ticket article attachment object.",
   "properties": {
      "Content": {
         "type": "string",
         "readOnly": true,
         "description": "added via include \"Content\" (base64 encoded)"
      },
      "ContentAlternative": {
         "type": "string",
         "readOnly": true
      },
      "ContentID": {
         "type": "string",
         "readOnly": true
      },
      "ContentType": {
         "type": "string",
         "readOnly": true
      },
      "Disposition": {
         "type": "string",
         "readOnly": true
      },
      "Filename": {
         "type": "string",
         "readOnly": true
      },
      "Filesize": {
         "type": "string",
         "readOnly": true
      },
      "FilesizeRaw": {
         "type": "string",
         "readOnly": true
      },
      "ID": {
         "type": "integer",
         "readOnly": true
      }
   },
   "required" : [
      "ContentAlternative",
      "ContentID",
      "ContentType",
      "Disposition",
      "Filename",
      "Filesize",
      "FilesizeRaw",
      "ID"
   ]
}
